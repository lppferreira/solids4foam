FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            ("libsampling.so");

application     solids4Foam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.05;

deltaT          1e-07;

maxCo           0.7;

writeControl    runTime;

writeInterval   0.01;

cycleWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

functions
{
    probe
    {
        type           probes;
        outputControl  timeStep;
        outputInterval 1;
        fields         (D);
        probeLocations ((0.45 0.45 0));
    }
}

// ************************************************************************* //
