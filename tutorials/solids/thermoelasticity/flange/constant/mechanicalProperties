FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress no;

mechanical
(
    steel
    {
        type            thermoLinearElastic;
        rho             rho [1 -3 0 0 0 0 0] 7800;
        E               E [1 -1 -2 0 0 0 0] 200e+09;
        nu              nu [0 0 0 0 0 0 0] 0.3;
        alpha           alpha [0 0 0 -1 0 0 0] 1e-6;
        T0              T0 [0 0 0 1 0 0 0] 273;
    }
);


