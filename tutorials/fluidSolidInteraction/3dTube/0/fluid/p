FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    symmetry-x
    {
        type        symmetryPlane;
    }
    symmetry-y
    {
        type        symmetryPlane;
    }
    outlet
    {
        type        fixedValue;
        value       uniform 0;
    }
    inlet
    {
        type       timeVaryingUniformFixedValue;
        fileName   "$FOAM_CASE/constant/fluid/time-series";
        outOfBounds clamp;
    }
    wall
    {
        // type       zeroGradient;
        type       extrapolatedPressure;
        value      uniform 0;
    }


