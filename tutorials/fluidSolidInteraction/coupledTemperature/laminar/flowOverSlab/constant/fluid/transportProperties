FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel Newtonian;

// Density
rho             rho [1 -3 0 0 0 0 0] 1.0;

// Laminar viscosity
nu              nu [0 2 -1 0 0 0 0] 0.001;

// Thermal expansion coefficient
beta            beta [0 0 0 -1 0 0 0] 0;

// Reference temperature
TRef            TRef [0 0 0 1 0 0 0] 300;

// Laminar Prandtl number
Pr              Pr [0 0 0 0 0 0 0] 0.01;

// Turbulent Prandtl number
Prt             Prt [0 0 0 0 0 0 0] 0.9;

// Heat capacity
// Note: ks/kf = 5 -->> Cp = 100.0
// Note: ks/kf = 20 -->> Cp = 25.0
Cp              Cp [0 2 -2 -1 0 0 0] 25.0;


