fluidSolidInterface    weakCoupling;//IQNILS;

//IQNILSCoeffs
weakCouplingCoeffs
{
    solidPatch         inner-wall;
    solidZone          inner-wall-zone;

    fluidPatch         walls;
    fluidZone          wall-zone;

    relaxationFactor   0.05;

    interfaceDeformationLimit 0;

    outerCorrTolerance 1e-6;

    nOuterCorr         30;

    interpolatorUpdateFrequency 1;

    couplingReuse       1;

    coupled             yes;
    coupledStartTime    0.1;
}

