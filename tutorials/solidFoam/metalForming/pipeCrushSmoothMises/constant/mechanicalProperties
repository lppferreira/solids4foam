FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    location    "constant";
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     no;

mechanical
(
    steel
    {
        type            neoHookeanElasticSmoothMisesPlastic;
        rho             rho [ 1 -3 0 0 0 0 0 ] 7800;
        E               E [ 1 -1 -2 0 0 0 0 ] 1.86e+11;
        nu              nu [ 0 0 0 0 0 0 0 ] 0.3;

        // Rate parameter (visco-plasticity)
        a0              0; //1.0;
        a1              0; //2.0;

        // Rate-independent parameters
        // These govern the amount of over-stress
        b0              0.0;
        b1              1000.0;

        // Starting value of kappa
        // This is the elastic strain at first yield
        kappa           0.001297400563;
        //kappa           0.01;

        // Rate at which kappas is approached
        m               0.1;
        //m               0.0;

        // Asymptotic value of kappa
        // It is a measure of elastic strain when the asymptotic yield stress is
        // reached
        kappas          0.007969746318;
        //kappas          0.01;
    }
);


